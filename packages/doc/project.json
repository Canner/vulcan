{
  "name": "doc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn docusaurus build",
        "cwd": "packages/doc"
      },
      "dependsOn": [
        {
          "target": "install-dependencies"
        }
      ]
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn docusaurus start",
        "cwd": "packages/doc"
      },
      "dependsOn": [
        {
          "target": "install-dependencies"
        }
      ]
    },
    "install-dependencies": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn",
        "cwd": "packages/doc"
      }
    }
  }
}
